    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "http-server": "^14.1.1",
    "jest": "^29.0.2",
    "jest-puppeteer": "^9.0.0",
    "puppeteer": "^20.8.2",
    "start-server-and-test": "^2.0.0",
    "terminate": "^2.6.1"
