{
  "name": "storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bump": "yarn workspace namada run bump --target storybook",
    "release": "release-it --verbose --ci",
    "release:dry-run": "release-it --verbose --dry-run --ci",
    "release:no-npm": "release-it --verbose --no-npm.publish --ci",
    "watch:tailwind": "npx tailwindcss -i ./src/tailwind-input.css -o ./src/styles/tailwind-output.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "build:tailwind": "npx tailwindcss -i ./src/tailwind-input.css -o ./src/styles/tailwind-output.css",
    "storybook": "concurrently 'yarn:watch:*'",
    "build-storybook": "yarn build:tailwind && storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@namada/components": "0.2.1",
    "@namada/types": "0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-themes": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "storybook": "^7.6.4",
    "tailwindcss": "^3.3.6",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
